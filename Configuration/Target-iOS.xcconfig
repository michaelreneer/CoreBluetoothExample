// Architectures
ARCHS = $(ARCHS_STANDARD)
SDKROOT = iphoneos

// Build Options
ENABLE_BITCODE = NO

// Code Signing
CODE_SIGN_IDENTITY[sdk=iphoneos*] = iOS Developer
PROVISIONING_PROFILE =

// Deployment
IPHONEOS_DEPLOYMENT_TARGET = 8.0
TARGETED_DEVICE_FAMILY = 1,2
